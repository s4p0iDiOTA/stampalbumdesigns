# ==============================================
# ENDICIA SHIPPING CONFIGURATION
# ==============================================
# 
# Add these variables to your .env file to enable
# Endicia shipping rate calculations
#
# Get credentials from: https://www.endicia.com/developer
# ==============================================

# API Endpoint
# Test: https://elstestserver.endicia.com/LabelService/EwsLabelService.asmx
# Production: https://labelserver.endicia.com/LabelService/EwsLabelService.asmx
ENDICIA_API_URL=https://elstestserver.endicia.com/LabelService/EwsLabelService.asmx

# Your Endicia Account Credentials
ENDICIA_ACCOUNT_ID=your_account_id_here
ENDICIA_PASS_PHRASE=your_pass_phrase_here

# Warehouse/Origin ZIP Code
ENDICIA_FROM_ZIP=90210

# Test Mode (true for test server, false for production)
ENDICIA_TEST_MODE=true
